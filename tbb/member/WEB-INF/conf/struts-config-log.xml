<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC
  "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
  "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
<struts-config>	

	<action-mappings>
		
		<!-- =============Example============== 
		<action path="/module/FunctionAction" type="com.tbb.module.action.FunctionAction">
			<forward name="redoQuery" path="/module/FunctionAction.do?method=query" redirect="true"/>
			<forward name="query" path="module.function.list" />
			<forward name="add" path="module.function.add" />
			<forward name="edit" path="module.function.edit" />
			<forward name="view" path="module.function.view" />
		</action>
		-->
		
		<action path="/log/IndexAction" 
			type="com.tbb.log.action.IndexAction">
			<forward name="view" path="log.index.view" />
		</action>
		
		<action path="/log/UserLogAction" type="com.tbb.log.action.UserLogAction">
			<forward name="query" path="log.user_log.list" />
			<forward name="view" path="log.user_log.view" />
		</action>
		
		<action path="/log/FhActionLogAction" type="com.tbb.log.action.FhActionLogAction">
			<forward name="query" path="log.fh_action_log.list" />
			<forward name="view" path="log.fh_action_log.view" />
		</action>
		
		<action path="/log/FeActionLogAction" type="com.tbb.log.action.FeActionLogAction">
			<forward name="query" path="log.fe_action_log.list" />
			<forward name="view" path="log.fe_action_log.view" />
		</action>
		
		<action path="/log/EquActionLogAction" type="com.tbb.log.action.EquActionLogAction">
			<forward name="query" path="log.equ_action_log.list" />
			<forward name="view" path="log.equ_action_log.view" />
		</action>
		
		<action path="/log/TeleLogAction" type="com.tbb.log.action.TeleLogAction">
			<forward name="query" path="log.tele_log.list" />
			<forward name="view" path="log.tele_log.view" />
		</action>
		
		<action path="/log/TeleBlackLogAction" type="com.tbb.log.action.TeleBlackLogAction">
			<forward name="query" path="log.tele_black_log.list" />
			<forward name="view" path="log.tele_black_log.view" />
		</action>
		
		<action path="/log/CaseLogAction" type="com.tbb.log.action.CaseLogAction">
			<forward name="query" path="log.case_log.list" />
			<forward name="view" path="log.case_log.view" />
		</action>
		
		<action path="/log/AttemperLogAction" type="com.tbb.log.action.AttemperLogAction">
			<forward name="query" path="log.attemper_log.list" />
			<forward name="view" path="log.attemper_log.view" />
		</action>
		
	</action-mappings>	
</struts-config>