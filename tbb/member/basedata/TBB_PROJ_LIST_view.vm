<form name="objForm" method="post">

    <!-- 隐含域 start -->
    <input type="hidden" name="method" >
    $!params.getHiddenAll() 
    <!-- 隐含域 end -->
	
	<table class="maintb">
		<tr><th colspan="10">详细信息</th></tr>    
		<tr>
		<tr>
			<td class="label">项目清单ID</td>
			<td><input name="ID" type="text" class="text"  value="$!dictionary.getID()" maxlength="20" readonly/></td>			
		</tr>
		<tr>
			<td class="label">章次</td>
			<td><input name="ZC" type="text" class="text"  value="$!dictionary.getZC()" maxlength="20" readonly/></td>			
		</tr>
		<tr>
			<td class="label">科目名称</td>
			<td><input name="KMMC" type="text" class="text"  value="$!dictionary.getKMMC()" maxlength="20" readonly/></td>			
		</tr>
		<tr>
			<td class="label">原合同金额</td>
			<td><input name="YHTJE" type="text" class="text"  value="$!dictionary.getYHTJE()" maxlength="20" readonly/></td>			
		</tr>
		<tr>
			<td class="label">变更金额</td>
			<td><input name="BGJE" type="text" class="text"  value="$!dictionary.getBGJE()" maxlength="20" readonly/></td>			
		</tr>
		<tr>
			<td class="label">总金额</td>
			<td><input name="ZJE" type="text" class="text"  value="$!dictionary.getZJE()" maxlength="20" readonly/></td>			
		</tr>
		<tr>
			<td class="label">基础信息ID</td>
			<td><input name="BASEDATA_ID" type="text" class="text"  value="$!dictionary.getBASEDATA_ID()" maxlength="20" readonly/></td>			
		</tr>
		<tr>
			<th colspan="10" class="op">
				<input class="button" type="button" onClick="history.back()" value="返回" />
			</th>
		</tr>
	</table>

</form>

